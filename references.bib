	file = {C\:\\Users\\m.el-masri\\Zotero\\storage\\9J6QENQC\\Chatterjee et Diaconis - 2015 - The sample size required in importance sampling.pdf},
	journal = {arXiv:1511.01437 [physics, stat]},
